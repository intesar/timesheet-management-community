<?xml version="1.0" encoding="UTF-8"?>
<persistence version="1.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd">
  <persistence-unit name="TrainingSchedular-DAOPU" transaction-type="RESOURCE_LOCAL">
    <provider>oracle.toplink.essentials.PersistenceProvider</provider>
    <class>com.abbhsoft.trainingschedular.model.Address</class>
    <class>com.abbhsoft.trainingschedular.model.Course</class>
    <class>com.abbhsoft.trainingschedular.model.Phone</class>
    <class>com.abbhsoft.trainingschedular.model.Class</class>
    <class>com.abbhsoft.trainingschedular.model.Trainees</class>
    <class>com.abbhsoft.trainingschedular.model.Prerequisite</class>
    <properties>
      <property name="toplink.jdbc.user" value="ts"/>
      <property name="toplink.jdbc.password" value="ts"/>
      <property name="toplink.jdbc.url" value="jdbc:oracle:thin:@localhost:1521:xe"/>
      <property name="toplink.jdbc.driver" value="oracle.jdbc.OracleDriver"/>
    </properties>
  </persistence-unit>
</persistence>
